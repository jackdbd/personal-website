<ul class="cluster" data-align="{{ align }}">
  <li><a href="{{ feed.articles.href }}">Articles feed</a></li>
  <li><a href="{{ feed.bookmarks.href }}">Bookmarks feed</a></li>
  <li><a href="{{ feed.likes.href }}">Likes feed</a></li>
  <li><a href="{{ feed.notes.href }}">Notes feed</a></li>
  <li><a href="{{ feed.talks.href }}">Talks feed</a></li>
  {% for entry in socials %}
  <li>
    <a href="{{ entry.url | url }}" rel="noopener noreferrer" target="_blank">{{ entry.name }}</a>
  </li>
  {% endfor %}
</ul>
