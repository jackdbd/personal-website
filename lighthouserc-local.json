{
  "ci": {
    "assert": {
      "budgetsFile": "./lighthouse/budgets.json"
    },
    "collect": {
      "numberOfRuns": 1,
      "settings": {
        "budgetsPath": "./lighthouse/budgets.json",
        "configPath": "./lighthouse/config/desktop.js"
      },
      "staticDistDir": "./_site",
      "url": [
          "http://localhost:8080",
          "http://localhost:8080/blog/"
        ]
    },
    "upload": {
      "target": "filesystem"
    }
  }
}
